    // This uses loacl USB-COM for Debugging a OBC "Layer3 - Debug UART" Protocol.
    "DEBUG-OBC": {
      "Impl": "StatusConsole.ObcDebugCli",
      "ImplConfigSection": "ClimbObcConfig",
      "ComName": "COM30",
      "Baud": "9600",
      "NewLine": "\n",
      "Screen": {
        "Width": 120,
        "Height": 20,
        "Pos_X": 0,
        "Pos_Y": 0,
