          },
          {
            "Val": 4,
            "Name": "ADO_SDC_CARDSTATUS_INIT_ACMD41_1"
          },
          {
            "Val": 5,
            "Name": "ADO_SDC_CARDSTATUS_INIT_ACMD41_2"
          },
          {
