        //"ADO_SDC_CARDSTATUS_WRITE_BUSYWAIT",
        //"ADO_SDC_CARDSTATUS_WRITE_CHECKSTATUS",
        //"ADO_SDC_CARDSTATUS_ERROR" "=", 99: null,
